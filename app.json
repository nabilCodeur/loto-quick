{
  "expo": {
    "name": "loto-quick",
    "slug": "loto-quick",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/dice-test-splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#14532d"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.nabilcodeur.lotoquick"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
      "react-native-google-mobile-ads",
      {
        "androidAppId":"ca-app-pub-7151192490184268~3187457653",
        "iosAppId":"ca-app-pub-xxxxxxxx~xxxxxxxx"
      }
    
    ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "abaef3c8-c43e-4cc9-9afa-72296dfb37f9"
      }
    }
  }
}
